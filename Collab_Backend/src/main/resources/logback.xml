<configuration>
  <appender name="FILE" class="ch.qos.logback.core.FileAppender">
    <file>D:/projects/Collab_Backend/logs/application.log</file>
    <encoder>
      <pattern>%d{dd-MM-yyyy: HH:mm:ss} - %p - %logger{1}:%msg%n</pattern>
    </encoder>
  </appender>
  <root level="debug">
    <appender-ref ref="FILE" />
  </root>
</configuration>